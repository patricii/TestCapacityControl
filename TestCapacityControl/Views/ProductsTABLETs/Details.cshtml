@model TestCapacityControl.Models.ProductsTABLET

@{
    ViewData["Title"] = "Details";
}

<h2>Resume Details</h2>

<div>
    <h4>Product: @Html.DisplayFor(model => model.ProductName)</h4>
    <hr />
    <dl class="dl-horizontal"></dl>
</div>
<table class="table">
    <thead>
        <tr>
            <th>
                <h3>ME</h3>
            </th>
            <th>
                <h3>BE</h3>
            </th>
            <th>
                <h3>CFC</h3>
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                <h5>IFLASH:<b> @Html.DisplayFor(model => model.YieldInline) %</b></h5>
                <h5>WIFI:<b> @Html.DisplayFor(model => model.YieldWifi) %</b></h5>
                <h5>RFCAL:<b> @Html.DisplayFor(model => model.YieldRFCal) %</b></h5>
                <h5>NSFT:<b> @Html.DisplayFor(model => model.YieldNSFT) %</b></h5>
            </td>
            <td>
                <h5>RADCURR:<b> @Html.DisplayFor(model => model.YieldRadCurr) %</b></h5>
                <h5>VISION TOF:<b> @Html.DisplayFor(model => model.YieldTOF) %</b></h5>
                <h5>ANT:<b> @Html.DisplayFor(model => model.YieldANT) %</b></h5>
