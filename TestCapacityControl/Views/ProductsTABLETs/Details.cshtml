            <td>
                <h5>WRITE NUMBER:<b> @Html.DisplayFor(model => model.YieldWriteNumber) %</b></h5>
                <h5>CFC UPGRADE:<b> @Html.DisplayFor(model => model.YieldCFCUp) %</b></h5>
                <h5>MMI 3:<b> @Html.DisplayFor(model => model.YieldMMI3) %</b></h5>
            </td>
        </tr>

    </tbody>
</table>
